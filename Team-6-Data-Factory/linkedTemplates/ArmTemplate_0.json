				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "HttpServerLocation",
						"relativeUrl": "/query?function=TIME_SERIES_DAILY&symbol=LAZR&outputsize=full&apikey=5OPV9WQVZ8HF8F8S"
