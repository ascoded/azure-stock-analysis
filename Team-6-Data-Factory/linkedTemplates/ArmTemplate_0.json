			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "NewsAPI1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "concat('AAPL', '_', formatDateTime(utcNow(), 'yyyyMMdd'), '.csv')\n",
							"type": "Expression"
						},
						"folderPath": "Bronze/AAPL",
						"fileSystem": "storage"
					}
				},
				"schema": {}
			},
