		"NewsAPI_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'NewsAPI'"
		},
		"NewsAPI1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'NewsAPI1'"
		},
		"AAPL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.alphavantage.co/query?function=NEWS_SENTIMENT&tickers=AAPL&apikey=5OPV9WQVZ8HF8FS&INTERVAL=60min"
		},
